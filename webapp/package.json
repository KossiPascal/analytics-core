    "dotenv": "^16.4.5",
    "eurodigit": "^3.1.3",
    "jquery": "^3.7.1",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "messageformat": "^2.3.0",
    "moment": "^2.30.1",
