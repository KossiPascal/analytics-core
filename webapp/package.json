  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=true",
    "clean": "npm cache clean --force",
    "serve": "npm run clean && ng serve --configuration production",
    "build": "rm -rf ./.angular && rm -rf ../views && npm run clean && ng build --service-worker --optimization --configuration production --aot --output-hashing=all --named-chunks --stats-json",
    "simple-build": "ng build --service-worker --optimization --configuration production --aot --output-hashing=all --named-chunks --stats-json",
    "compile": "ngc",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "precache": "sw-precache --verbose --config=sw-precache-config.js"
  },
  "private": true,
  "prettier": {
    "singleQuote": true,
    "printWidth": 100,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "parser": "typescript"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/cdk": "^17.3.4",
